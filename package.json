{
  "name": "example-app",
  "version": "1.0.0",
  "description": "Example of Angular development using JSPM.",
  "main": "app.js",
  "directories": {
    "doc": "docs"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "eslint": "^2.13.1",
    "eslint-config-singpath": "file:packages/eslint-config-singpath",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-html": "^1.5.1",
    "http-server": "^0.9.0",
    "istanbul": "^0.4.3",
    "jspm": "^0.17.0-beta.17",
    "mocha": "^2.5.3",
    "pre-commit": "^1.1.3",
    "remap-istanbul": "^0.6.4",
    "source-map-explorer": "^1.3.2",
    "source-map-support": "^0.4.0"
  },
  "jspm": {
    "name": "example-app",
    "main": "example-app.js",
    "dependencies": {
      "angular-route": "github:angular/bower-angular-route@^1.5.6",
      "angular-rx-subscribe": "npm:angular-rx-subscribe@^0.2.5",
      "css": "github:systemjs/plugin-css@^0.1.22",
      "dirty-chai": "npm:dirty-chai@^1.2.2",
      "get-parameter-names": "npm:get-parameter-names@^0.3.0",
      "rx-firebase": "npm:rx-firebase@^0.4.0",
      "rxjs": "npm:rxjs@5.0.0-beta.8",
      "sinon-chai": "npm:sinon-chai@^2.8.0",
      "text": "github:systemjs/plugin-text@^0.0.8"
    },
    "devDependencies": {
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.12"
    },
    "peerDependencies": {
      "angular": "github:angular/bower-angular@^1.5.6",
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "chai": "npm:chai@^3",
      "firebase": "github:firebase/firebase-bower@3",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha",
      "sinon": "npm:sinon@^1.17.4",
      "util": "github:jspm/nodelibs-util@^0.2.0-alpha"
    },
    "overrides": {
      "github:angular/bower-angular-route@1.5.6": {
        "main": "angular-route",
        "registry": "jspm",
        "dependencies": {
          "angular": "^1.3.0"
        },
        "meta": {
          "angular-route.js": {
            "deps": [
              "angular"
            ],
            "format": "global"
          }
        }
      },
      "github:angular/bower-angular@1.5.6": {
        "main": "angular",
        "meta": {
          "angular.js": {
            "exports": "angular",
            "format": "global"
          }
        }
      },
      "npm:inherits@2.0.1": {
        "ignore": [
          "test.js"
        ]
      }
    }
  },
  "scripts": {
    "build": "bash ./tools/bin/build.sh",
    "clean": "bash ./tools/bin/clean",
    "cover": "bash ./tools/bin/cover.sh",
    "lint": "eslint src/ packages/ index.html --fix",
    "lint-no-fix": "eslint src/ index*.js",
    "postinstall": "jspm install",
    "start": "http-server -c-1 -p 8000",
    "start:build": "http-server dist/ -c-1 -p 8000",
    "test": "bash ./tools/bin/test.sh"
  },
  "pre-commit": [
    "lint-no-fix"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dinoboff/example-app.git"
  },
  "keywords": [],
  "author": "Chris Boesch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dinoboff/example-app/issues"
  },
  "homepage": "https://github.com/dinoboff/example-app#readme"
}

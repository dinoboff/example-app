{
  "name": "example-app",
  "version": "1.0.0",
  "description": "",
  "main": "src/example.js",
  "scripts": {
    "build:test": "rm -rf dist/ && mkdir -p dist/ && jspm build ./tests.js dist/test.js --skip-rollup --format cjs",
    "clean": " rm -rf dist/",
    "postinstall": "jspm install",
    "start": "python -m SimpleHTTPServer 8000",
    "test": "npm run build:test && mocha --require source-map-support/register dist/test.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "jspm": "^0.17.0-beta.17",
    "mocha": "^2.5.3",
    "source-map-support": "^0.4.0"
  },
  "jspm": {
    "name": "example-app",
    "main": "example.js",
    "dependencies": {
      "angular": "github:angular/bower-angular@1.5",
      "chai": "npm:chai@^3.5.0"
    },
    "devDependencies": {
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.11"
    },
    "peerDependencies": {
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha"
    },
    "overrides": {
      "github:angular/bower-angular@1.5.6": {
        "main": "angular",
        "meta": {
          "angular.js": {
            "exports": "angular",
            "format": "global"
          }
        }
      }
    }
  }
}
